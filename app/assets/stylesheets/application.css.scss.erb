  }

a {
  color: #FC0;
  &:visited {
    color: #FC0; }
  &:hover {
    color: yellow;
    } }

#header h1 a,
h1 a,
h1 a:visited,
.trailer a,
.trailer a:visited {
  color: $creme; text-decoration: none;
  }
#header h1 a:hover,
h1 a:hover,
.trailer a:hover {
  color: darken($creme,5%); text-decoration: underline;
  }

#header {
  position: relative;
  left: 15px; top: 5px;
  }
h1 {
  display: inline;
  font: {
    family: museo-slab, verdana, sans-serif;
    weight: 900;
    style: italic;
  }
  letter-spacing: -1px;
  }

h1#winner-title {font-style: normal;}
h2 {
  font: {
    family: museo-slab, verdana, sans-serif;
    weight: 700;
    size: 0.9em;
  }
  display: inline;
  padding-left: 25px;
}

.winner-description p {padding-left: 20px; line-height: 1.4em; font-weight: bold;}
#winner-title {font-size: 1.4em; font-weight: bold;}
#winner > .info-area {
  background-color: darken($theatre_red, 15%);
  @include radial-gradient(180, 45, 45, 70, 9, 10);
  @include border-radius(5px);
  @include box-shadow(15px, #000);
  @include padding-margin(10px, 20px);
  width: 500px;
  min-height: 320px;
  }
#winner #winner-poster img {
  @include light-box-shadow(20px,30%);
  @include border-radius(3px);
  border: 5px solid $border_color;
  width: 180px;
  margin: 5px 15px 5px 0px;
  float: left;
  }
#winner #winner-poster img:hover {
  @include light-box-shadow(20px,50%);
  }
.info-area:hover {
  cursor: pointer;
}

.imdb, .video {position:relative; top:4px; padding-right: 4px;}
#winner .imdb, #winner .video {top:3px;}

.score, .trailer {
  font-size: 0.8em;
  font-weight: bold;
  color: darken($creme, 5%);
  padding: 5px;
  background-color: #a51c1a;
  @include linear-gradient(165, 28, 26, 72, 20, 21);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#a51c1a', EndColorStr='#481415');
  @include box-shadow(2px, #000);
  @include border-radius(5px);
  text-shadow: 0px 0px 5px #000;
  }

.disc-description {
  font-size: 0.8em;
  font-weight: normal;
  color: darken($creme, 5%);
  padding: 5px;
  background-color: #a51c1a;
  @include linear-gradient(165, 28, 26, 72, 20, 21);
  filter: progid:DXImageTransform.Microsoft.gradient(startColorStr='#a51c1a', EndColorStr='#481415');
  @include box-shadow(2px, #000);
  @include border-radius(5px);
  text-shadow: 0px 0px 5px #000;
}

#runners-up {
  font-size: 0.7em;
  line-height: 18px;
  }
#runners-up h1 {font-weight: 700; font-style: normal; font-size: 1.9em;}
#runners-up > .info-area {
  @include radial-gradient(142, 26, 26, 13, 15, 17);
  @include border-radius(5px);
  @include box-shadow(6px, #000);
  width: 400px;
  min-height: 170px;
  background-color: darken($theatre_red, 20%);
  @include padding-margin(10px, 20px);
  }

#runners-up-poster {margin: 0px 0px 0px 5px;}

#runners-up #runners-up-poster img {
  @include border-radius(3px);
  @include box-shadow(10px, lighten($theatre_red, 30%));
  border: 5px solid $border_color;
  float: right;
  width: 100px;
  }
#runners-up #runners-up-poster img:hover {
  @include box-shadow(10px, lighten($theatre_red, 50%));
  }

.upcoming-movie {
  color: darken($creme, 5%);
  // font-weight: bold;
  padding: 10px;
  margin: 10px 20px 10px 20px;
  background-color: #a51c1a;
  @include radial-gradient(142, 26, 26, 13, 15, 17);
  @include border-radius(5px);
  @include box-shadow(6px, #000);
  text-shadow: 0px 0px 5px #000;
  max-width: 540px;
  min-height: 75px;
  line-height: 1em;
  }
.upcoming-movie:hover {
  cursor: pointer;
  @include box-shadow(9px, #000);
  @include radial-gradient(132, 16, 16, 3, 5, 7);
}

.upcoming-movie a { color: $creme; text-decoration: none; }
.upcoming-poster { float: left; }
.title-and-date {font-weight: 700; font-style: normal; line-height: 18px;}
.upcoming-date { display: block; postition: relative; margin-top: 10px; }

.upcoming-poster {
  @include border-radius(3px);
  @include box-shadow(10px, lighten($theatre_red, 30%));
  border: 1px solid $border_color;
  float: left;
  padding: 0px; margin: 0px 10px 0px 0px;
  }
.upcoming-poster:hover {
  @include box-shadow(10px, lighten($theatre_red, 50%));
  }

.menu  {
  color: darken($creme, 10%);
  padding: 5px;
  margin: 20px;
  background-color: #C80000;
  @include radial-gradient(200, 0, 0, 55, 0, 0);
  @include border-radius(5px);
  @include box-shadow(6px, #000);
  text-shadow: 0px 0px 5px #000;
  max-width: 540px;
  line-height: 1em;
}
.menu h1 {
  font-size: 1.5em;
  font-style: normal;
  font-weight: bold;
}
ul {
  font: {
    family: museo-slab, verdana, sans-serif;
    weight: 400;
    size: 1.4em;
  }
  line-height: 1.5em;
  margin: 0px;
}

#footer {
  display: block;
  font: {
    family: museo-slab, verdana, sans-serif;
    weight: 700;
    size: 0.7em;
  };
  position: relative;
  left: 15px; top: 5px;
  margin-bottom: 50px;
  }

.footer-link { font-size: 1.75em; display: block; }